= form_for @user_vote_city do |f|
  -if @user_vote_city.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_vote_city.errors.count, "error")} prohibited this user_vote_city from being saved:"
      %ul
        - @user_vote_city.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :city
    = f.text_field :city
  .field
    = f.label :state
    = f.text_field :state
  .field
    = f.label :country
    = f.text_field :country
  .field
    = f.label :latitude
    = f.text_field :latitude
  .field
    = f.label :longitude
    = f.text_field :longitude
  .actions
    = f.submit 'Save'
