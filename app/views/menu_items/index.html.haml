= render :partial => "/restaurant_head"
-#- @menu_item = @restaurant
#content_wrap
  #content
    #current_content_block
      #bread_crumbs
        =link_to("← "+"back to results", "#")
      #current_info_wrapper.restaurant{:data => {:latitude=> @restaurant.latitude, :longitude => @restaurant.longitude}}
        #img_column.himportant
          =image_tag "temp_image.png"
          #icons_img_wrapper
            %span.photos_inline
              =link_to "75 photos","@@"
            %span.add_photos_inline
              =link_to "add a photo","@@"
            #photos_icon &nbsp;
        #text_column.restaurant
          %h1.current_title= @restaurant.name

          #desc_wrap_rest
            #description
              %p= @restaurant.street_address
              %p= @restaurant.neighborhood
              %p= "#{@restaurant.city}, #{@restaurant.state} #{@restaurant.zip}"



              .phone_number
                .col1_pn
                  %p (415) 927 1849
                  %p= link_to "www.siet.com"

                .col2_pn= image_tag "social.png"


    #reviews_wrapper.restaurant
      %h3.review
        Menu

      #select_box_hint &nbsp;
      #sorting
        #before_sort_by
          Sort by:
        #sort_by
          =text_field(:select, :sorttype, :value => "Newest", :disabled => true)
          %ul
            -["Rating", "Other"].each do |x|
              %li= x
      #update_place_restaurants
        .star_sm.stars_3 &nbsp;
        - @menu_items.each do |@mi|
          = render :partial => "menu_item"




  #sidebar
    #map{:style => "width:297px; height:297px; border: 1px solid #ccc"}
      =image_tag "map_temp.png"
    #foodies_wraper_wrapper
      %h4 Foodies of the week
      -#-for @user in @foodies_for_current_dish
      -#  =render :partial => "foodies"


#footer
  %p footer

